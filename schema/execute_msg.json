{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ExecuteMsg",
  "oneOf": [
    {
      "type": "object",
      "required": [
        "onboard_asset"
      ],
      "properties": {
        "onboard_asset": {
          "type": "object",
          "required": [
            "asset_type",
            "identifier",
            "validator_address"
          ],
          "properties": {
            "asset_type": {
              "type": "string"
            },
            "identifier": {
              "$ref": "#/definitions/AssetIdentifier"
            },
            "validator_address": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "validate_asset"
      ],
      "properties": {
        "validate_asset": {
          "type": "object",
          "required": [
            "identifier",
            "success"
          ],
          "properties": {
            "identifier": {
              "$ref": "#/definitions/AssetIdentifier"
            },
            "message": {
              "type": [
                "string",
                "null"
              ]
            },
            "success": {
              "type": "boolean"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "add_asset_definition"
      ],
      "properties": {
        "add_asset_definition": {
          "type": "object",
          "required": [
            "asset_definition"
          ],
          "properties": {
            "asset_definition": {
              "$ref": "#/definitions/AssetDefinitionInput"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "update_asset_definition"
      ],
      "properties": {
        "update_asset_definition": {
          "type": "object",
          "required": [
            "asset_definition"
          ],
          "properties": {
            "asset_definition": {
              "$ref": "#/definitions/AssetDefinitionInput"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "toggle_asset_definition"
      ],
      "properties": {
        "toggle_asset_definition": {
          "type": "object",
          "required": [
            "asset_type",
            "expected_result"
          ],
          "properties": {
            "asset_type": {
              "type": "string"
            },
            "expected_result": {
              "type": "boolean"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "add_asset_validator"
      ],
      "properties": {
        "add_asset_validator": {
          "type": "object",
          "required": [
            "asset_type",
            "validator"
          ],
          "properties": {
            "asset_type": {
              "type": "string"
            },
            "validator": {
              "$ref": "#/definitions/ValidatorDetail"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "update_asset_validator"
      ],
      "properties": {
        "update_asset_validator": {
          "type": "object",
          "required": [
            "asset_type",
            "validator"
          ],
          "properties": {
            "asset_type": {
              "type": "string"
            },
            "validator": {
              "$ref": "#/definitions/ValidatorDetail"
            }
          }
        }
      },
      "additionalProperties": false
    }
  ],
  "definitions": {
    "AssetDefinitionInput": {
      "description": "Allows the user to optionally specify the enabled flag on an asset definition, versus forcing it to be added manually on every request, when it will likely always be specified as `true`.",
      "type": "object",
      "required": [
        "asset_type",
        "scope_spec_address",
        "validators"
      ],
      "properties": {
        "asset_type": {
          "type": "string"
        },
        "enabled": {
          "type": [
            "boolean",
            "null"
          ]
        },
        "scope_spec_address": {
          "type": "string"
        },
        "validators": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ValidatorDetail"
          }
        }
      }
    },
    "AssetIdentifier": {
      "oneOf": [
        {
          "type": "object",
          "required": [
            "asset_uuid"
          ],
          "properties": {
            "asset_uuid": {
              "type": "object",
              "required": [
                "asset_uuid"
              ],
              "properties": {
                "asset_uuid": {
                  "type": "string"
                }
              }
            }
          },
          "additionalProperties": false
        },
        {
          "type": "object",
          "required": [
            "scope_address"
          ],
          "properties": {
            "scope_address": {
              "type": "object",
              "required": [
                "scope_address"
              ],
              "properties": {
                "scope_address": {
                  "type": "string"
                }
              }
            }
          },
          "additionalProperties": false
        }
      ]
    },
    "Decimal": {
      "description": "A fixed-point decimal value with 18 fractional digits, i.e. Decimal(1_000_000_000_000_000_000) == 1.0\n\nThe greatest possible value that can be represented is 340282366920938463463.374607431768211455 (which is (2^128 - 1) / 10^18)",
      "type": "string"
    },
    "FeeDestination": {
      "type": "object",
      "required": [
        "address",
        "fee_percent"
      ],
      "properties": {
        "address": {
          "type": "string"
        },
        "fee_percent": {
          "$ref": "#/definitions/Decimal"
        }
      }
    },
    "Uint128": {
      "description": "A thin wrapper around u128 that is using strings for JSON encoding/decoding, such that the full u128 range can be used for clients that convert JSON numbers to floats, like JavaScript and jq.\n\n# Examples\n\nUse `from` to create instances of this and `u128` to get the value out:\n\n``` # use cosmwasm_std::Uint128; let a = Uint128::from(123u128); assert_eq!(a.u128(), 123);\n\nlet b = Uint128::from(42u64); assert_eq!(b.u128(), 42);\n\nlet c = Uint128::from(70u32); assert_eq!(c.u128(), 70); ```",
      "type": "string"
    },
    "ValidatorDetail": {
      "type": "object",
      "required": [
        "address",
        "fee_destinations",
        "fee_percent",
        "onboarding_cost",
        "onboarding_denom"
      ],
      "properties": {
        "address": {
          "type": "string"
        },
        "fee_destinations": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/FeeDestination"
          }
        },
        "fee_percent": {
          "$ref": "#/definitions/Decimal"
        },
        "onboarding_cost": {
          "$ref": "#/definitions/Uint128"
        },
        "onboarding_denom": {
          "type": "string"
        }
      }
    }
  }
}
