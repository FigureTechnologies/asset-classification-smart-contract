        "contract_upgrade"
      ],
      "properties": {
        "contract_upgrade": {
          "type": "object",
          "properties": {
            "options": {
              "anyOf": [
                {
                  "$ref": "#/definitions/MigrationOptions"
                },
                {
                  "type": "null"
                }
              ]
            }
          }
        }
      },
      "additionalProperties": false
    }
  ],
  "definitions": {
    "MigrationOptions": {
      "description": "Sub-level struct that defines optional changes that can occur during the migration process.",
